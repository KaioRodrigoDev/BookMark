{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Assets/*": ["./src/assets/*"],
      "@Components/*": ["./src/components/*"],
      "@Containers/*": ["./src/containers/*"],
      "@Contexts/*": ["./src/contexts/*"],
      "@Hooks/*": ["./src/hooks/*"],
      "@Icons/*": ["./src/icons/*"],
      "@Pages/*": ["./src/pages/*"],
      "@Routes/*": ["./src/routes/*"],
      "@Services/*": ["./src/services/*"],
      "@Tests/*": ["./src/tests/*"],
      "@Types/*": ["./src/types/*"],
    }
  },
}
